 def four(userinput):
    if userinput.find("i") < userinput.find("e"):
        if userinput.find("c") < userinput.find("i"):
            print ("False")
            return False
        else:
            print("True")
            return True
    elif userinput.find("e") < userinput.find("i"):
        if userinput.find("c") < userinput.find("e"):
            print ("True")
            return True
        else:
            print ("False")
            return False
___________________________________________________

def five(userinput):
    answer = 1
    for i in range(1,userinput+1):
        answer = answer*i
    print (answer)
    return answer

