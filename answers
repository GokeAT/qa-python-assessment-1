
def one(input1,input2):
    if len(input1) > len(input2):
        print (input1)
        return input1
    elif len(input2) > len(input1):
        print (input2)
        return input2
    else:
        print (input1 + " " +input2)
        return input1 + " " +input2
____________________________________________________

def two(arg1):
    if arg1%3 == 0 and arg1%5 == 0:
        print ("fizzbuzz")
        return "fizzbuzz"
    elif arg1%3 == 0:
        print ("fizz")
        return "fizz"
    elif arg1%5 == 0:
        print ("buzz")
        return "buzz"
    else:
        print ("null")
        return "null"

____________________________________________________

def three(userinput):
    countvowel = 0
    vowels = ["A", "a","E","e","I","i","O","o","U","u"]
    for char in userinput:
        if char in vowels:
            countvowel = countvowel + 1
    print (countvowel)
    return countvowel
  
